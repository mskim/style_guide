<div id="yh_picture_search_form">
  <%= render 'search_form' %>
</div>
<div class="row mt-4 mb-5">
  <div class="col-sm-8">
    <div id="yh_picture_search_result">
      <%= render 'search_result' %>
    </div>
    <div id="yh_picture_paginate">
      <%= render 'search_result_paginate' %>
    </div>
  </div>
  <div class="col-sm-4" id="yh_picture_preview">
    <%= render 'preview' %>
  </div>
  <br>
</div>
<nav class="navbar fixed-bottom navbar-light bg-light">
  <ul class="nav">
    <li class="nav-item">
      <%= link_to "이전으로", yh_pictures_path, method: :get, class: "btn btn-info mx-1" %>
    </li>
    <li class="nav-item">
      <%= link_to "사진선택", taken_yh_picture_path, method: :get, class: "btn btn-info" %>
    </li>
  </ul>
</nav>
